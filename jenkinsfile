pipeline {
    agent any
    tools { 
      maven 'Maven3.8.7' 
      jdk 'openjdk' 
    }
    stages {
        stage("git clone"){
            steps{
                git branch: 'main', url: 'https://github.com/NarenPrakash/thymeleafExample.git'
            }
        }    
        stage("build code"){
            steps{
                bat 'mvn clean install'
            }
        }
    }
}
